{
	"manifest_version" : 3,
	"name" : "Gradescope Grade Calculator",
	"version" : "1.03",
	"content_scripts": [ {
    "matches": ["https://www.gradescope.com/courses/*"],
    "js": ["content.js"]
  } ],

    "icons": {
      "128": "Calculator.png"
    },

    "action" : { 
      "default_icon" : "Calculator.png",
      "default_popup" : "index.html",
      "default_title" : "Popup"
    }
}